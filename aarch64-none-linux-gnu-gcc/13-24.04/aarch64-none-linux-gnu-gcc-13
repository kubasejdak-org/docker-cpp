LINUX_ARM_TOOLCHAIN_PATH=/opt/toolchains/arm-gnu-toolchain-13.3.rel1-x86_64-aarch64-none-linux-gnu

[settings]
os=Linux
os_build=Linux
arch=armv8
arch_build=x86_64
compiler=gcc
compiler.version=13
compiler.libcxx=libstdc++11
build_type=Release

[env]
AR=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-ar
AS=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-gcc
CC=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-gcc
CXX=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-g++
LD=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-ld
OBJCOPY=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-objcopy
RANLIB=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-ranlib
SIZE=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-size
STRIP=$LINUX_ARM_TOOLCHAIN_PATH/bin/aarch64-none-linux-gnu-strip
